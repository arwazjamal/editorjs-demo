{
  "name": "editorjs-demo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:prod": "rm -r dist && mkdir dist && webpack && npm run copy-files",
    "copy-files": "cp src/index.html dist && cp src/json-preview.js dist && cp src/demo.css dist",
    "build:dev": "npm run start",
    "start": "webpack-dev-server",
    "rename-list": "node -e \"require('fs').rename('dist/bundle.js', 'dist/list_bundle.js', function(err) { if (err) console.log(err); })\"",
    "rename-table": "node -e \"require('fs').rename('dist/bundle.js', 'dist/table_bundle.js', function(err) { if (err) console.log(err); })\"",
    "build-list": "cp node_modules/editorjs-list/dist/bundle.js dist && npm run rename-list",
    "build-table": "cp node_modules/editorjs-table/dist/bundle.js dist && npm run rename-table"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@editorjs/header": "^2.6.0",
    "editorjs-list": "^2.1.2",
    "editorjs-simple-image-updated": "^1.3.3",
    "editorjs-table": "^1.4.3",
    "editorjs-updated": "^2.19.3",
    "i": "^0.3.6",
    "npm": "^6.14.8"
  },
  "devDependencies": {
    "@editorjs/checklist": "^1.3.0",
    "@editorjs/code": "^2.6.0",
    "@editorjs/delimiter": "^1.2.0",
    "@editorjs/embed": "^2.4.0",
    "@editorjs/inline-code": "^1.3.1",
    "@editorjs/link": "^2.3.1",
    "@editorjs/marker": "^1.2.2",
    "@editorjs/quote": "^2.4.0",
    "@editorjs/raw": "^2.2.0",
    "@editorjs/simple-image": "^1.4.0",
    "@editorjs/warning": "^1.2.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "webpack": "^5.4.0",
    "webpack-cli": "~3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
